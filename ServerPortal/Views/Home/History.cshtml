@model ServerPortal.Models.History

@{
    ViewBag.Title = "History";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>History</h2>

<table>
    <tr>
        <th>FileName</th>
        <th># Downloads</th>
    </tr>

@foreach (var item in Model.Downloads) {
    <tr>
        <td>
            @item.Name
        </td>
        <td>
            @item.DLCnt  
        </td>
    </tr>
}

</table>
